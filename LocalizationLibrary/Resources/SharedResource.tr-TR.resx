        <xsd:element name="root" msdata:IsDataSet="true">
            <xsd:complexType>
                <xsd:choice maxOccurs="unbounded">
                    <xsd:element name="data">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="value" type="xsd:string" minOccurs="0" />
                            </xsd:sequence>
                            <xsd:attribute name="name" use="required" type="xsd:string" />
                        </xsd:complexType>
                    </xsd:element>
                </xsd:choice>
            </xsd:complexType>
        </xsd:element>
    </xsd:schema>

    <data name="GreetingMessage" xml:space="preserve">
    <value>Merhaba! Sistemimize hoş geldin</value>
  </data>

    <data name="ErrorMessage" xml:space="preserve">
    <value>Beklenmedik bir hata oluştu. Litfen daha sonra tekrar deneyin.</value>
  </data>
    <data name="DynamicMessage" xml:space="preserve">
        <value>{0} değerinin minimum uzunluğu {1} olmalıdır.</value>
    </data>
</root>